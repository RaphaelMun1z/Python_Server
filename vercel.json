{
    "version": 2,
    "builds": [
        {
            "src": "./server_app.py",
            "use": "@vercel/python",
            "config": {
                "runtime": "python3.9"
            }
        }
    ],
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "./server_app.py"
        }
    ]
}